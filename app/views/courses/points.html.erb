<h1>Test Runner: Current points for <%= @course.name %></h1>

<% if signed_in? %>
    
<table class="tableListings" width="600">
  <tr>
    <th>Student number</th>
    <th>Course</th>
    <th>Exercise number</th>
    <th>Point to GDocs</th>
    <th>Upload status</th>
  </tr>
  
  <% @points.each do |point| %>
  <tr>
    <td><%= point.student_id %></td>
    <td><%= @course.name %></td>

    <td><%= point.exercise_number %></td>
    <td><%= point.tests_pass %></td>
    <td><%= queue_status point %></td>
  </tr>
 <% end %>
</table>

<% else %>

  <a href="https://docs.google.com">Go to Google docs to see your points.</a>

<% end %>

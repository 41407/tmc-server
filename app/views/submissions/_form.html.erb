<%= form_for(submission, :url => course_exercise_submissions_path(@course, @exercise), :action => @form_action,
            :html => {:multipart => true}) do |f| %>
  <%= render 'shared/error_messages', :target => submission %>

  <div>

  </div>
  <%= labeled_field("Zipped project", f.file_field('file')) %>
  <div class="actions">
    <%= f.submit 'Submit'%>
  </div>
<% end %>
